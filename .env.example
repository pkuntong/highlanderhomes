# Example environment variables file (DO NOT store actual secrets in this file)
# Copy this file to .env and fill in your actual values

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# RentCast API Configuration
VITE_RENTCAST_API_KEY=your_rentcast_api_key_here

# Stripe Configuration (use test keys for development)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
# NOTE: Secret key should NEVER be in frontend code - use a backend/serverless function
# STRIPE_SECRET_KEY=your_stripe_secret_key_here

# Plaid Configuration (sandbox environment)
VITE_PLAID_CLIENT_ID=your_plaid_client_id_here
VITE_PLAID_ENV=sandbox
# NOTE: Plaid secret should NEVER be in frontend code - use a backend/serverless function
# PLAID_SECRET=your_plaid_secret_here
